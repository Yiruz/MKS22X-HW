public class MyLinkedList{
    LNode start;
    int size;

    public boolean add(int value){
	LNode current = start;
	while(current != null){
	    current = current.getNext();
	    return false;
	}
        current.setLNode(value);
	return true;
    }
    public String toString(){
	String s = "[";
	LNode current = start;
	for(int i=0; i<size; i++){
	    s += current.getLNode() + ",";
	}
	s+="]";
	return s;
    }

    private class LNode{
	int data;
	LNode next;

	public LNode(int d, LNode n){
	    data = d;
	    next = n;
	}
	public int getData(){
	    return data;
	}
	public LNode getLNode(){
	    return next;
	}
	public void setData(int d){
	    data = d;
	}
	public void setLNode(LNode n){
	    next = n;
	}
    }

}
